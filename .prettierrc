{
  "tabWidth": 2,
  "semi": true,
  "singleQuote": false,
  "importOrder": [
    "^react$",
    "next",
    "^next/(.*)$",
    "^@tabler/(.*)$",
    "^@prisma/(.*)$",
    "clsx",
    "currency.js",
    "dayjs",
    "^@/app/(.*)$",
    "^@/components/(.*)$",
    "^@/lib/(.*)$",
    "^@/utils",
    "^@/hooks",
    "^@/constants",
    "^[a-zA-Z0-9@](.*)$",
    "^[./]"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true,
  "plugins": [
    "@trivago/prettier-plugin-sort-imports"
  ]
}